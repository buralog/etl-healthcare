{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ingest.raw.v1",
  "type": "object",
  "required": ["type", "tenantId", "ingestId", "contentType", "s3", "receivedAt", "traceId"],
  "properties": {
    "type": { "const": "ingest.raw.v1" },
    "tenantId": { "type": "string" },
    "ingestId": { "type": "string" },
    "contentType": { "type": "string" },
    "s3": {
      "type": "object",
      "required": ["bucket", "key"],
      "properties": {
        "bucket": { "type": "string" },
        "key": { "type": "string" }
      }
    },
    "source": { "type": "string" },
    "receivedAt": { "type": "string", "format": "date-time" },
    "traceId": { "type": "string" }
  },
  "additionalProperties": false
}
