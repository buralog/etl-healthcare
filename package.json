{
  "name": "etl-healthcare",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "synth": "cdk synth",
    "deploy:dev": "cdk deploy --all --require-approval never",
    "bootstrap": "cdk bootstrap",
    "gen:types": "json2ts -i libs/contracts/schemas/**/*.json -o libs/contracts/src/types.ts",
    "publish:schemas": "bash scripts/publish-schemas.sh"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.896.0",
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/client-sqs": "^3.896.0",
    "@aws-sdk/lib-dynamodb": "^3.896.0",
    "@aws-sdk/client-lambda": "^3.896.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "aws-cdk-lib": "^2.154.0",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@types/node": "^22.18.6",
    "esbuild": "^0.24.0",
    "json-schema-to-typescript": "^15.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
